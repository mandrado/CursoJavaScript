<%- include('includes/head') %>


    <section class="container">
        <h1>
            <%= umaVariavelLocal %>
                <br>
                <br>
                <!-- usar %= mantem o texto sem respeitar as tags -->
                <%= titulo %>
                    <br>
                    <br>
                    <!-- usar %- respeita as tags do texto  -->
                    <%- titulo %>
                        <!-- usando linguagem javascript no ejs -->
                        <% if (typeof titulo !=='undefined' ) { %>
                            <br>
                            <%- titulo %>
                                <br>
                                <% } else { %>
                                    <br>
                                    O título não foi enviado.
                                    <br>
                                    <% } %>

                                        <% numeros.forEach(num=> { %>
                                            <%- num %>
                                                <% }) %>

        </h1>

        <form action="/" method="post">
            <label for="nome-cliente">Cliente</label>
            <input type="text" id="nome-cliente" name="cliente">
            <button>Enviar</button>
        </form>
    </section>

    <%- include('includes/footer') %>